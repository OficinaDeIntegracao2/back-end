{
  "dependencies": {
    "@prisma/client": "6.8.2",
    "bcrypt": "6.0.0",
    "cors": "2.8.5",
    "data-fns": "1.1.0",
    "date-fns": "4.1.0",
    "dotenv": "16.4.5",
    "express": "4.19.2",
    "glob": "10.3.16",
    "helmet": "7.1.0",
    "jsonwebtoken": "9.0.2",
    "pino": "9.1.0",
    "pino-pretty": "11.1.0",
    "reflect-metadata": "0.2.2",
    "swagger-jsdoc": "6.2.8",
    "swagger-ui-express": "5.0.1",
    "tsyringe": "4.8.0"
  },
  "devDependencies": {
    "@types/bcrypt": "5.0.2",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/glob": "8.1.0",
    "@types/jest": "29.5.14",
    "@types/jsonwebtoken": "9.0.9",
    "@types/swagger-jsdoc": "6.0.4",
    "@types/swagger-ui-express": "4.1.8",
    "jest": "29.7.0",
    "prisma": "6.8.2",
    "rimraf": "5.0.7",
    "ts-jest": "29.3.4",
    "ts-node-dev": "2.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.4.5"
  },
  "engines": {
    "node": ">=20.13.1 <=20.13.1"
  },
  "prisma": {
    "seed": "ts-node  --transpile-only src/configuration/database/seed.ts"
  },
  "main": "dist/main.js",
  "name": "back-end",
  "scripts": {
    "setup": "npx prisma migrate dev && npx prisma generate",
    "build": "tsc",
    "clean": "rimraf ./dist",
    "dev": "ts-node-dev --pretty --respawn --transpile-only -r tsconfig-paths/register ./src/start.ts | pino-pretty",
    "start:dev": "npm run setup && npm run dev",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "version": "1.0.0"
}
